#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_webdriver-manager@12.1.8@webdriver-manager/bin/webdriver-manager" "$@"
  ret=$?
else 
  node  "$basedir/../../../_webdriver-manager@12.1.8@webdriver-manager/bin/webdriver-manager" "$@"
  ret=$?
fi
exit $ret
