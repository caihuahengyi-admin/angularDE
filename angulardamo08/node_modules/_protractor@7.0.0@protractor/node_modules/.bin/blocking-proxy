#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_blocking-proxy@1.0.1@blocking-proxy/built/lib/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_blocking-proxy@1.0.1@blocking-proxy/built/lib/bin.js" "$@"
  ret=$?
fi
exit $ret
