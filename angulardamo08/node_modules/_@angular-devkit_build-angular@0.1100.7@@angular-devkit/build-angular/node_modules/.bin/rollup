#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_rollup@2.32.1@rollup/dist/bin/rollup" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_rollup@2.32.1@rollup/dist/bin/rollup" "$@"
  ret=$?
fi
exit $ret
